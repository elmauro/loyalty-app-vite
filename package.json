{
  "name": "loyalty-app-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cy:dev": "cross-env VITE_USE_MSW=true vite",
    "cy:e2e": "start-server-and-test cy:dev http://localhost:51730 \"cypress open\"",
    "cy:pre:e2e": "npx --yes kill-port 51730 || exit 0",
    "cy:e2e:run": "npm run cy:pre:e2e && start-server-and-test cy:dev http://localhost:51730 \"cypress run --e2e\"",
    "cy:e2e:run:rules": "npm run cy:pre:e2e && start-server-and-test cy:dev http://localhost:51730 \"cypress run --e2e --spec cypress/e2e/rules.cy.ts\"",
    "cy:dev:real": "vite",
    "cy:e2e:real": "start-server-and-test cy:dev:real http://localhost:51730 \"cypress open\"",
    "cy:e2e:run:real": "start-server-and-test cy:dev:real http://localhost:51730 \"cypress run --e2e\""
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "amazon-cognito-identity-js": "^6.3.16",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cypress": "^14.3.0",
    "cypress-vite": "^1.6.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.4",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.24.1",
    "util": "^0.12.5",
    "vite": "^6.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
